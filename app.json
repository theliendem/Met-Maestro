{
  "expo": {
    "name": "Met Maestro",
    "slug": "met-maestro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./met-maestro-logo.png",
    "scheme": "metmaestro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theliendem.metmaestro",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./met-maestro-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [],
      "versionCode": 1,
      "package": "com.theliendem.metmaestro"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./met-maestro-logo.png"
    },
    "updates": {
      "channel": "production"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e7392638-783c-4262-a78a-3f6968051904"
      }
    }
  }
}