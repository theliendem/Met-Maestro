{
  "expo": {
    "name": "Met Maestro",
    "slug": "met-maestro",
    "version": "4.2",
    "orientation": "portrait",
    "icon": "./met-maestro-logo.png",
    "scheme": "metmaestro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theliendem.metmaestro",
      "buildNumber": "16",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "Met Maestro needs microphone access to detect pitch for the tuner feature."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./met-maestro-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "versionCode": 18,
      "package": "com.theliendem.metmaestro"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./met-maestro-logo.png"
    },
    "updates": {},
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio",
      "expo-asset",
      "expo-document-picker",
      [
        "react-native-permissions",
        {
          "ios": {
            "microphone": "Met Maestro needs microphone access to detect pitch for the tuner feature."
          },
          "android": {
            "microphone": "Met Maestro needs microphone access to detect pitch for the tuner feature."
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e7392638-783c-4262-a78a-3f6968051904"
      }
    }
  }
}